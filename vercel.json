{
  "version": 2,
  "routes": [
    {
      "src": "/multiplayer/([^/]+)",
      "dest": "/multiplayer/[roomId]"
    },
    {
      "src": "/play/([^/]+)",
      "dest": "/play/[roomId]"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "regions": ["iad1"]
}